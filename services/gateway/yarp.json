{
  "ReverseProxy": {
    "Routes": {
      "Admin Service": {
        "ClusterId": "sysCluster",
        "Match": {
          "Path": "/api/sys/{**everything}"
        },
        "Transforms": [
          { "PathPattern": "{**everything}" }
        ]
      },
      "Demo Service": {
        "ClusterId": "demoCluster",
        "Match": {
          "Path": "/api/demo/{**everything}"
        },
        "Transforms": [
          { "PathPattern": "{**everything}" }
        ]
      }
    },
    "Clusters": {
      "sysCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:44326"
          }
        }
      },
      "demoCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:44327"
          }
        }
      }

    }
  }
}